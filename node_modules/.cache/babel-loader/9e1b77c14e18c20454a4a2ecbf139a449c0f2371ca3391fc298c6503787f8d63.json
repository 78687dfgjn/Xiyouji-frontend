-else class=\"el-icon-plus avatar-uploader-icon\"></i>\r\n      </el-upload>\r\n      <el-form-item label=\"昵称\" prop=\"adminName\">\r\n        <el-input v-model=\"ruleForm.nickname\" clearable></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"账号\" prop=\"adminAccount\">\r\n        <el-input v-model=\"ruleForm.account\" clearable></el-input>\r\n      </el-form-item>\r\n\r\n      <el-form-item label=\"手机号码\" prop=\"contactNumber\">\r\n        <el-input v-model=\"ruleForm.phoneNumber\" clearable></el-input>\r\n      </el-form-item>\r\n      <el-form-item label=\"邮箱\" prop=\"adminEmail\">\r\n        <el-input v-model=\"ruleForm.email\" clearable></el-input>\r\n      </el-form-item>\r\n\r\n      <!-- <el-form-item label=\"用户性别\" prop=\"sex\">\r\n        <template>\r\n          <el-radio v-model=\"ruleForm.sex\" label=\"1\">男</el-radio>\r\n          <el-radio v-model=\"ruleForm.sex\" label=\"0\">女</el-radio>\r\n        </template>\r\n      </el-form-item> -->\r\n\r\n      <el-form-item label=\"所属地区\" prop=\"region\">\r\n        <el-cascader\r\n            v-model=\"districtValue\"\r\n            placeholder=\"请选择所属地区\"\r\n            :options=\"districtOptions\"\r\n            filterable\r\n        ></el-cascader>\r\n      </el-form-item>\r\n\r\n      <!-- <el-form-item label=\"生日\">\r\n        <el-col :span=\"11\">\r\n          <el-form-item prop=\"birthday\">\r\n            <el-date-picker\r\n              type=\"date\"\r\n              placeholder=\"选择日期\"\r\n              v-model=\"ruleForm.birthday\"\r\n              style=\"width: 100%\"\r\n            ></el-date-picker>\r\n          </el-form-item>\r\n        </el-col>\r\n      </el-form-item> -->\r\n      <el-form-item>\r\n        <el-button type=\"primary\" @click=\"submitForm('ruleForm')\"\r\n        >确认修改\r\n        </el-button\r\n        >\r\n        <el-button @click=\"handleCancel\">取消</el-button>\r\n      </el-form-item>\r\n    </el-form>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexpo